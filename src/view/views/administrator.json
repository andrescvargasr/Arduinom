                "var OpenBio=require.node('./../devices/OpenBio');\nvar OpenSpectro=require.node('./../devices/OpenSpectro');\n\nvar deviceArray= API.cache('deviceArray');\nvar deviceList= API.cache('deviceList');\nvar devicekey=get('devicekey');\nif(devicekey){\n    var device=deviceList[devicekey];\n    var deviceInfo=device.paramInfo;\n    device.getParsedCompactLog().then((value)=> {\n        for(let key in deviceInfo){\n            //console.log(value[0].parameters[deviceInfo[key].parameter]);\n\n            if(value[0].parameters[deviceInfo[key].parameter]){\n                deviceInfo[key].val=value[0].parameters[deviceInfo[key].parameter];\n            }\n        \n        }\n     API.createData(\"deviceInfo\",deviceInfo);\n\n        \n    });\n}\n"
