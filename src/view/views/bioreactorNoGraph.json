    {
      "url": "modules/types/client_interaction/code_executor/",
      "configuration": {
        "sections": {},
        "groups": {
          "group": [
            {
              "display": [
                [
                  "buttons",
                  "editor"
                ]
              ],
              "execOnLoad": [
                []
              ],
              "asyncAwait": [
                []
              ],
              "script": [
                "var OpenBio=require.node('./../devices/OpenBio/OpenBio');\nvar OpenSpectro=require.node('./../devices/OpenSpectro/OpenSpectro');\n\nvar deviceArray= API.getData('deviceArray');\nvar deviceList= API.cache('deviceList');\nvar devicekey=get('devicekey');\nvar device\n\nif(devicekey){\n    device=deviceList[String(devicekey)];\n    \n    switch(this.button){\n        case 'reload':\n            reload().catch(errorHandler);\n            device.getHelp().then((value)=>{API.createData('helpMenu',value)});\n            break;\n        case 'setTime':\n            setTimeNow().catch(errorHandler);\n            break;\n        case 'entryID':\n            lastEntry().catch(errorHandler);\n            break;\n        case 'last':\n            lastLog().catch(errorHandler);\n            break;\n        case 'lasts':\n            lastLogs().catch(errorHandler);\n            break;\n        default:\n            reload().catch(errorHandler);\n            device.getHelp().then((value)=>{API.createData('helpMenu',value)});\n            break;\n            \n        \n    }\n        \n}\nelse ui.showNotification('no device selected', 'error');\n\n//Utilities\nfunction lastLogs(){\n    return device.getLastEntryID().then((val)=> {\n        return device.getMultiLog(val-10).then((buff)=>{\n                    ui.showNotification('got Last Logs \\n' +buff , 'success');\n         API.createData(\"lastCmdExec\",buff);\n        })\n\n    })\n}\n\nfunction lastLog(){\n    return device.getLastLog().then((val)=> {\n        ui.showNotification('got Last Log: ' + val, 'success');\n         API.createData(\"lastCmdExec\",val);\n    })\n}\n\nfunction lastEntry(){\n    return device.getLastEntryID().then((val)=> {\n        ui.showNotification('got Last EntryID: ' + val, 'success');\n         API.createData(\"lastCmdExec\",val);\n    })\n}\n\nfunction setTimeNow(){\n    return device.setEpochNow().then((val)=> {\n        ui.showNotification('Time was set to Unix epoch', 'success');\n         API.createData(\"lastCmdExec\",val);\n    })\n}\n\nfunction reload(){\n    var deviceInfo=device.paramInfo;\n    return device.getParsedCompactLog().then((value)=> {\n        for(let key in deviceInfo){\n            if(value.parameters[deviceInfo[key].parameter]){\n                deviceInfo[key].val=value.parameters[deviceInfo[key].parameter];\n            }\n        \n        }\n     API.createData(\"deviceInfo\",deviceInfo);\n    })\n    \n}\n\nfunction errorHandler(err){\n    ui.showNotification(err.message, 'error');\n}\n\n\n\n\n"
              ]
