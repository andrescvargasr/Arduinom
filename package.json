{
  "name": "arduinom",
  "version": "0.0.0",
  "description": "node interface for at32u4",
  "main": "src/",
  "dependencies": {
    "bson": "^0.5.7",
    "debug": "^2.2.0",
    "deepcopy": "^0.6.3",
    "express": "^4.14.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "https": "^1.0.0",
    "open-spectro": "0.0.7",
    "path": "^0.12.7",
    "pouchdb": "^6.0.5",
    "pouchdb-find": "^0.10.3",
    "serial-requests": "^0.1.3",
    "socket.io": "^1.6.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "eslint": "^3.9.1",
    "eslint-config-cheminfo": "^1.5.2",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "node-gyp": "^3.4.0",
    "watchify": "^3.8.0"
  },
  "scripts": {
    "browser-build": "mkdir -p lib/public/browserified; browserify src/client/index.js -o lib/public/browserified/index.js --standalone=arduinom --global-transform [ babelify --sourceMap ]",
    "browser-watch": "mkdir -p lib/public/browserified; watchify src/client/index.js -v -o lib/public/browserified/index.js --standalone=arduinom --global-transform [ babelify --sourceMap ]",
    "build": "npm run browser-build && npm run node-build",
    "eslint": "eslint src",
    "eslint-fix": "eslint src --fix",
    "node-build": "babel src --out-dir lib --quiet",
    "node-watch": "npm run node-build -- --watch",
    "prepublish": "npm run build",
    "start": "node lib/server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "serial",
    "arduino"
  ],
  "author": "qcabrol",
  "license": "MIT"
}
