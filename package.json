{
  "name": "serialarduino",
  "version": "0.0.1",
  "description": "node interface for at32u4",
  "main": "src/",
  "dependencies": {
    "debug": "^2.2.0",
    "deepcopy": "^0.6.3",
    "open-spectro": "0.0.7",
    "pouchdb": "^6.0.5",
    "pouchdb-find": "^0.10.3",
    "serial-requests": "^0.0.1"
  },
  "devDependencies": {
    "electron-prebuilt": "1.2.1",
    "eslint": "^3.9.1",
    "eslint-config-cheminfo": "^1.5.2",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "node-gyp": "^3.4.0"
  },
  "scripts": {
    "prepublish":"npm run browserify",
    "postinstall": "npm run build-serialport && npm run build-leveldown",
    "build-serialport": "node node_modules/node-gyp/bin/node-gyp.js rebuild --target=1.2.1 --arch=x64 --directory=./node_modules/serialport --dist-url=https://atom.io/download/atom-shell",
    "build-leveldown": "node node_modules/node-gyp/bin/node-gyp.js rebuild --target=1.2.1 --arch=x64 --directory=./node_modules/leveldown --dist-url=https://atom.io/download/atom-shell",
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify src/ws_client/index.js -o src/public/browserified/index.js --standalone=arduino",
    "start": "electron ./src/view",
    "eslint": "eslint src",
    "eslint-fix": "eslint src --fix"
  },
  "keywords": [
    "serial",
    "arduino"
  ],
  "author": "qcabrol",
  "license": "MIT"
}
